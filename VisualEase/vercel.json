{
    "version": 2,
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "frontend/build"
        }
      },
      {
        "src": "backend/GroqAPI/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "backend/api/Image_Generator.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/app1(.*)",
        "dest": "backend/GroqAPI/app.py"
      },
      {
        "src": "/app2(.*)",
        "dest": "backend/api/Image_Generator.py"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
  